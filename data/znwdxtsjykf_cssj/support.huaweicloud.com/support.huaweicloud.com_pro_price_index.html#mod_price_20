<style type="text/css">
    #videolist div div
    {
        text-align: center;
        background-repeat: no-repeat;
    }
    #videolist div div img
    {
        margin-top: 25px;
        cursor: pointer;
    }
</style>
<div id="videoDiv" style="z-index:9999;display:none;position:relative;">
    <div id="videobg" style="position:absolute;background:#000;opacity:0.3;filter: progid:DXImageTransform.Microsoft.Alpha(opacity=30);">
    </div>
    <div id="videoplayerDiv" style="position:absolute;overflow:hidden;">
        <div id="videoContainer" style="width:700px;height:500px;background-color: black;">
            <span id="closeBtn"><span style="position:absolute;right:0;cursor:pointer;z-index: 10000;"> <img src="//img.huaweicloud.com/static/images/v1/close_grey.png" alt="" height="22" width="22" onclick="closeVideo()" /> </span></span>
            <div id="videoplayer" style="height:500px;width:700px;">
            </div>
        </div>
    </div>
</div>
<script src="//img.huaweicloud.com/static/js/v1/swfobject.js"></script>
<script type="text/javascript">
    var videoDomain = '//video.huaweicloud.com';
    var staticDomain = '//static.huaweicloud.com';
    
    var _videoProtocol = (("https:" == document.location.protocol) ? "https:" : "http:");
    function play(video_url,video_desc,domain){
        if(domain){
            var tmp = _videoProtocol + domain + video_url;
        }
        else{
            var tmp = _videoProtocol + videoDomain + video_url;
        }
        tmp = tmp.replace(/m3u8/, 'mp4');

        //ga('send','event','video','click',video_desc);

        var ua = navigator && navigator.userAgent;
        if ((window.applicationCache) || ua.toLowerCase().indexOf('android') > -1 || (/iPad/i).test(ua) || (/iPhone/i).test(ua)) {
            var win_width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            var obj = "#videoplayer";
            if(win_width < 700)
            {
                $("#videoContainer").width(win_width).height(win_width*3/4);
                $(obj).width(win_width).height(win_width*3/4);
            }
            var h = $(obj).height();
            var w = $(obj).width();

            $(obj).append('<video src="'+tmp+'"  height="'+h+'" width="'+w+'" autoplay="autoplay" controls="controls"></video>');
        }
        else
        {
            var parameters = {
                src: tmp,
                autoPlay: "true",
                verbose: true,
                controlBarAutoHide: "true",
                controlBarPosition: "bottom",
                hls_minbufferlength: -1,
                hls_maxbufferlength: 10,
                hls_lowbufferlength: 3,
                hls_seekmode: "KEYFRAME",
                hls_startfromlevel: -1,
                hls_seekfromlevel: -1,
                hls_live_flushurlcache: false,
                hls_info: false,
                hls_debug: false,
                hls_debug2: false,
                hls_warn: false,
                hls_error: false,
                hls_fragmentloadmaxretry : -1,
                hls_manifestloadmaxretry : -1,
                hls_capleveltostage : false,
                hls_maxlevelcappingmode : "downscale"
            };
            swfobject.embedSWF(
                _videoProtocol + staticDomain + "/static/swf/StrobeMediaPlayback.swf"
                , "videoplayer"
                , 700
                , 500
                , "10.1.0"
                , "expressInstall.swf"
                , parameters
                , {
                    allowFullScreen: "true"
                }
            );
        }


        var userAgent = navigator.userAgent;
        var isOpera = userAgent.indexOf("Opera") > -1;
        if (userAgent.indexOf("Firefox") > -1) {
            $("#videoplayer").css("padding-top","30px");
            $("#videoContainer").css("height","470px");
        }
        if (!!window.ActiveXObject || "ActiveXObject" in window){
            $("#videoplayer").css("padding-top","30px");
        }
        var height = document.body.offsetHeight;
        $("#videobg").css("width", $(window).width());
        $("#videobg").css("height", height);
        $("#videobg").css("top", -height);
        $("#videobg").css("left", "0px");
        $("#videoplayerDiv").css("left", ($(window).width() - $("#videoplayer").width()) / 2);
        $("#videoplayerDiv").css("top", (($(window).height() - $("#videoplayer").height()) / 2 + $(window).scrollTop())-height);
        $("#videoDiv").show();
    }
    
    function closeVideo()
    {
        if (!!window.ActiveXObject || "ActiveXObject" in window){
            $("#videoplayer").remove();
            $("#closeBtn").append('<div id="videoplayer" style="height:500px;width:700px;">');

        }else
        {
            $("#videoplayer").html("");
        }
        $("#videoDiv").hide();
    }
    
</script>


