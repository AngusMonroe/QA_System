<script>
 var hot_ajax_url = 'https://portal.huaweicloud.com/api/search/hot';
var site_search_url = 'http://www.huaweicloud.com';
            
        $('.searchInput').keyup(function(){
            var SearchUrl = $(".search-inner").data("href");
            $('.search-inner').attr('href',SearchUrl+$.base64.urlSafeEncode($('.searchInput').val(),true))
        })
        $(".searchInput").keydown(function (event) {
            var e = event || window.event || arguments.callee.caller.arguments[0];
            if (e && e.keyCode == 13) {
                $(this).siblings(".search-inner")[0].click() 
            }
        })


